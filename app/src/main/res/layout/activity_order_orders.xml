<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fci.delite.OrderOrders">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="94dp"
        android:layout_height="43dp"
        android:text="total salary"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="201dp"
        android:layout_marginBottom="37dp"
        android:layout_above="@+id/textView5"
        android:layout_alignStart="@+id/textView4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="94dp"
        android:layout_height="43dp"
        android:text="time"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="269dp"
        android:layout_above="@+id/editText42"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="94dp"
        android:layout_height="43dp"
        android:text="typee catogary"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="135dp"
        android:layout_above="@+id/textView6"
        android:layout_alignStart="@+id/textView2"
        android:layout_marginBottom="43dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="94dp"
        android:layout_height="43dp"
        android:text="food"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="77dp"
        android:layout_marginStart="23dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="29dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="confirm"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="389dp"
        android:layout_above="@+id/button7"
        android:layout_alignStart="@+id/button7"
        android:layout_marginBottom="10dp" />

    <EditText
        android:id="@+id/editText41"
        android:layout_width="74dp"
        android:layout_height="38dp"
        android:ems="10"
        android:hint="12:25"
        android:inputType="time"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="274dp"
        android:layout_alignBottom="@+id/editText42"
        android:layout_toEndOf="@+id/textView2"
        android:layout_marginStart="14dp" />

    <EditText
        android:id="@+id/editText42"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="20/11/2017"
        android:inputType="date"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="269dp"
        android:layout_above="@+id/button4"
        android:layout_alignEnd="@+id/editText43"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="18dp" />

    <EditText
        android:id="@+id/editText43"
        android:layout_width="203dp"
        android:layout_height="179dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="77dp"
        android:layout_marginStart="11dp"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignStart="@+id/button4" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cancle"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="453dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp" />
</RelativeLayout>